  "author": "Joseph Scherben <jscherbe@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jscherbe/sassdoc-to-markdown/issues"
  },
  "homepage": "https://github.com/Jscherbe/sassdoc-to-markdown#readme",
  "devDependencies": {
    "@ulu/vitepress-auto-menus": "^0.0.3",
    "sass": "^1.60.0",
    "vitepress": "^1.0.0-rc.24"
  },
  "dependencies": {
    "@ulu/markdown-output-utils": "^0.0.6",
    "fs-extra": "^11.1.0",
    "sassdoc": "^2.7.4",
    "strip-indent": "^4.0.0",
    "yaml": "^2.2.1"
  },
  "peerDependencies": {
    "sass": ">1.50"
  }
