{
  "name": "@ulu/sassdoc-to-markdown",
  "version": "0.0.16",
  "description": "A flexible SCSS documentation tool that generates clean, customizable Markdown from your Sass stylesheets. It leverages SassDoc for data extraction and offers various configuration options to tailor the output to your specific needs",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jscherbe/sassdoc-to-markdown.git"
  },
  "scripts": {
    "test:dev": "vitepress dev test",
    "test:output": "node ./test/.vitepress/sassdoc/output.js",
    "test:output:inspect": "node --inspect-brk ./test/.vitepress/sassdoc/output.js"
  },
  "keywords": [
    "scss",
    "sassdoc",
    "markdown",
    "output",
    "sass",
    "documentation",
    "compiled examples",
    "documentation",
    "generator",
    "frontend development",
    "web development",
    "css",
    "documentation tool",
    "developer tool",
    "theming",
    "design system",
    "theme documentation"
  ],
  "author": "Joseph Scherben <jscherbe@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jscherbe/sassdoc-to-markdown/issues"
  },
  "homepage": "https://github.com/Jscherbe/sassdoc-to-markdown#readme",
  "devDependencies": {
    "@ulu/vitepress-auto-menus": "^0.0.3",
    "sass": "^1.60.0",
    "vitepress": "^1.0.0-rc.24"
  },
  "dependencies": {
    "@ulu/markdown-output-utils": "^0.0.6",
    "fs-extra": "^11.1.0",
    "sassdoc": "^2.7.4",
    "strip-indent": "^4.0.0",
    "yaml": "^2.2.1"
  },
  "peerDependencies": {
    "sass": ">1.50"
  }
}
